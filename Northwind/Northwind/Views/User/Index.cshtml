@model IEnumerable<Northwind.Models.NorthwindUser>

@{
    ViewData["Title"] = "Users";
}
@using Northwind.Helpers

<h1>Categories</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                UserName
            </th>
            <th>
                Email
            </th>
        </tr>
    </thead>
    <tbody>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.UserName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Email)
            </td>
        </tr>
    }
    </tbody>
</table>